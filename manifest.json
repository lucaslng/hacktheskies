{
  "name": "Placeholder",
  "description": "Placeholder side panel demo",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "UI/background.js"
  },
  "permissions": ["sidePanel", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "side_panel": {
    "default_path": "UI/sidepanel.html"
  }
}
